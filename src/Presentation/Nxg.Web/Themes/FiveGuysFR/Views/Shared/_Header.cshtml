@using Nop.Core
@using Nop.Services.Customers
@using Nop.Services.Orders
@using Nop.Web.Infrastructure
@using Nop.Core.Domain.Orders;
@using Nop.Core.Infrastructure
@using Nop.Services.Helpers
@using Nop.Web.Framework.TagHelpers.Shared
@inject IWebHelper _webHelper
@inject ICustomerService _customerService;
@inject IWorkContext _workContext
@inject IThemeContext _themeContext
@inject IShoppingCartService _shoppingCartService;
@inject IStoreContext _storeContext;
@{
    var themeName = await _themeContext.GetWorkingThemeNameAsync();
    var returnUrl = Context.Request.Query.TryGetValue("returnUrl", out var url) && !StringValues.IsNullOrEmpty(url)
                           ? url.ToString()
                           : _webHelper.GetRawUrl(Context.Request);
    var customer = await _workContext.GetCurrentCustomerAsync();
    var isAuthenticated = await _customerService.IsRegisteredAsync(customer);

    var cart = await _shoppingCartService.GetShoppingCartAsync(await _workContext.GetCurrentCustomerAsync(), ShoppingCartType.ShoppingCart, (await _storeContext.GetCurrentStoreAsync()).Id);
    var cartItemsCount = cart.Sum(x => x.Quantity);
}
